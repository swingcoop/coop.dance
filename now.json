{
   "version": 2,
   "alias": [ "swing.dance.coop" ],
   "builds": [
      { "src": "www/**", "use": "@now/static" },
      { "src": "api/reservations/index.js", "use": "@now/node" },
      { "src": "api/reservations/starter-kit/index.js", "use": "@now/node" },
      { "src": "api/letters-add.js", "use": "@now/node" },
      { "src": "api/messages-add.js", "use": "@now/node" },
      { "src": "api/notifications-send.js", "use": "@now/node" },
      { "src": "api/potluck-add.js", "use": "@now/node" },
      { "src": "api/potluck/get.js", "use": "@now/node" },
      { "src": "api/profile-get.js", "use": "@now/node" },
      { "src": "api/profile-update.js", "use": "@now/node" }
   ],
   "routes": [
      { "src": "/api/reservations", "dest": "/api/reservations/index.js" },
      { "src": "/api/reservations/starter-kit", "dest": "/api/reservations/starter-kit/index.js" },
      { "src": "/api/letters", "dest": "/api/letters-add.js" },
      { "src": "/api/messages", "methods": ["POST"], "dest": "/api/messages-add.js" },
      { "src": "/api/notifications/send", "dest": "/api/notifications-send.js" },
      { "src": "/api/potluck", "dest": "/api/potluck-add.js" },
      { "src": "/api/potluck/lessons", "dest": "/api/potluck/get.js" },
      { "src": "/api/profile", "methods": ["GET"], "dest": "/api/profile-get.js" },
      { "src": "/api/profile", "methods": ["PUT"], "dest": "/api/profile-update.js" },
      { "src": "/(.*)", "dest": "/www/$1" }
   ],
   "env": {
      "TWILIO_API_KEY": "@twilio-api-key",
      "TWILIO_ACCOUNT": "@twilio-account-sid",
      "TWILIO_MESSAGE_CLIENT": "@twilio-message-client",
      "AIRTABLE_API_KEY": "@airtable-api-key",
      "AIRTABLE_SUBSCRIPTIONS_BASE": "@airtable-subscriptions-base",
      "AUTH0_USERS_ID": "@auth0-users-client-id",
      "AUTH0_USERS_KEY": "@auth0-users-client-secret",
      "CLOUDKARAFKA_BROKERS": "@cloudkarafka-brokers",
      "CLOUDKARAFKA_USERNAME": "@cloudkarafka-username",
      "CLOUDKARAFKA_PASSWORD": "@cloudkarafka-password"
   }
}